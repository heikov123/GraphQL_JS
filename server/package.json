  },
  "homepage": "https://github.com/heikov123/GraphQL_JS#readme",
  "dependencies": {
    "apollo-server-express": "^2.9.13",
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "base-64": "^0.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "gravatar": "^1.8.0",
    "humps": "^2.0.1",
    "nodemon": "^2.0.2",
    "pg": "^7.14.0",
    "ramda": "^0.26.1",
    "striptags": "^3.1.1"
  }
}
