    "url": "https://github.com/heikov123/GraphQL_JS/issues"
  },
  "homepage": "https://github.com/heikov123/GraphQL_JS#readme",
  "dependencies": {
    "apollo-server-express": "^2.9.13",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6"
  }
}
